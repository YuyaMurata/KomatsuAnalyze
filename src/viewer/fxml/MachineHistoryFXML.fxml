<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="root_vbox" prefHeight="586.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewer.fxml.MachineHistoryFXMLController">
    <children>
        <MenuBar fx:id="fileMenu">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <SplitPane fx:id="history_splitpane" dividerPositions="0.5" VBox.vgrow="ALWAYS">
            <items>
                <ListView fx:id="machineList" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                          <Label fx:id="history_label" layoutX="15.0" layoutY="14.0" text="Label" textOverrun="CLIP" />
                  <ListView fx:id="pList" layoutX="14.0" layoutY="68.0" prefHeight="262.0" prefWidth="229.0" />
                  <ListView fx:id="cList" layoutX="251.0" layoutY="68.0" prefHeight="262.0" prefWidth="229.0" />
                  <TableView fx:id="sampleView" layoutX="15.0" layoutY="380.0" prefHeight="98.0" prefWidth="464.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="C1" />
                      <TableColumn prefWidth="75.0" text="C2" />
                    </columns>
                  </TableView>
                  <Button fx:id="applyButton" layoutX="414.0" layoutY="340.0" mnemonicParsing="false" onAction="#applyAction" prefHeight="31.0" prefWidth="63.0" text="âˆ¨" />
                  <Button fx:id="csv" layoutX="438.0" layoutY="482.0" mnemonicParsing="false" onAction="#writeCSV" text="CSV" />
                  <CheckBox fx:id="allCheck" layoutX="433.0" layoutY="42.0" mnemonicParsing="false" onAction="#allChecked" text="/ALL" />
                  <ChoiceBox fx:id="csvOutputForm" layoutX="316.0" layoutY="482.0" prefWidth="100.0" />
               </children>
            </AnchorPane>
            </items>
        </SplitPane>
      <Pane maxHeight="30.0" minHeight="30.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="csvWorkingLabel" layoutX="13.0" layoutY="4.0" prefHeight="21.0" prefWidth="193.0" />
         </children>
      </Pane>
    </children>
</VBox>
